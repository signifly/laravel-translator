{
    "name": "signifly/laravel-translator",
    "description": "Database based translations for your Eloquent models.",
    "homepage": "https://github.com/signifly/laravel-translator",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Translator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\Translator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Translator\\TranslatorServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
